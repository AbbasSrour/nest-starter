{
  "name": "nest-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "new": "hygen new",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:hmr": "node src/main.hmr.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "watch:dev": "ts-node-dev -r tsconfig-paths/register src/main.ts",
    "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
    "console": "node ./dist/console.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config jest.e2e.config.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "pnpm run typeorm migration:generate -d ormconfig src/database/migrations/default",
    "migration:create": "pnpm run typeorm migration:create src/database/migrations/default",
    "migration:run": "pnpm typeorm migration:run -d ormconfig.ts",
    "migration:revert": "pnpm run typeorm migration:revert",
    "schema:drop": "pnpm run typeorm schema:drop -d ormconfig.ts",
    "webpack": "webpack --config webpack.config.js --progress"
  },
  "dependencies": {
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/mime-types": "^2.1.1",
    "aws-sdk": "^2.1305.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express-ctx": "^0.1.1",
    "express-rate-limit": "^6.7.0",
    "inquirer": "^9.1.4",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "libphonenumber-js": "^1.10.18",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "nest-commander": "^3.4.0",
    "nest-winston": "^1.8.0",
    "nestjs-i18n": "^10.2.6",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.9",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.16",
    "@types/jest": "29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@types/webpack-env": "^1.18.0",
    "eslint": "^8.33.0",
    "eslint-config-custom": "github:ciatek/ciatek-eslint",
    "jest": "29.4.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.2",
    "typescript": "^4.9.5"
  }
}
